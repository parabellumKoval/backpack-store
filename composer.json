{
    "name": "parabellumkoval/backpack-store",
    "description": "Implement Products API & MYSQL for dackpack",
    "keywords": [
        "parabellumKoval",
        "backpack",
        "laravel",
        "products"
    ],
    "homepage": "https://github.com/parabellumkoval/products",
    "license": "MIT",
    "authors": [
      {
        "name": "parabellumKoval",
        "email": "parabellum.koval@gmail.com",
        "role": "Developer"
      }
    ],
    "require": {
      "php": "^7.3|^8.0",
      "laravel/framework": "^8.75",
      "backpack/crud": "4.1.*",
      "cviebrock/eloquent-sluggable": "^8.0",
      "spatie/laravel-translatable": "5.1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "*"
    },
    "autoload": {
      "psr-4": {
        "App\\": "src/app/",
        "Backpack\\Store\\": "src",
        "Backpack\\Store\\Database\\Factories\\": "database/factories"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "App\\": "src/app/",
        "Backpack\\Store\\Tests\\": "tests"
      }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint",
        "phpunit": "vendor/bin/phpunit --testdox"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
          "pestphp/pest-plugin": true,
          "phpstan/extension-installer": true
        }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Backpack\\CRUD\\BackpackServiceProvider",
          "Backpack\\Store\\ServiceProvider"
        ],
        "aliases": {
          "Product": "Backpack\\Store\\Facades\\Store",
          "CRUD": "Backpack\\CRUD\\app\\Library\\CrudPanel\\CrudPanelFacade"
        }
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
